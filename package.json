{
  "name": "@beorn/flexx",
  "version": "0.1.0",
  "description": "Pure JavaScript flexbox layout engine with Yoga-compatible API",
  "keywords": [
    "css",
    "flexbox",
    "layout",
    "pure-javascript",
    "terminal",
    "tui",
    "yoga",
    "zero-dependency"
  ],
  "homepage": "https://github.com/beorn/flexx#readme",
  "bugs": {
    "url": "https://github.com/beorn/flexx/issues"
  },
  "license": "MIT",
  "author": "beorn",
  "repository": {
    "type": "git",
    "url": "https://github.com/beorn/flexx.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./classic": {
      "types": "./dist/index-classic.d.ts",
      "import": "./dist/index-classic.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "bench": "bunx --bun vitest bench",
    "bench:standalone": "bun bench/run.ts",
    "bench:compare": "bun bench/compare.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "debug": "^4.4.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/debug": "^4.1.12",
    "typescript": "^5.7.3",
    "yoga-wasm-web": "^0.3.3"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18"
  }
}
